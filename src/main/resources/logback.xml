<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{YYYY-MM-dd HH:mm:ss.SSS} [%thread] %-5level %logger{36} - %msg%n</pattern>
        </encoder>
    </appender>
    <root level="trace">
        <appender-ref ref="STDOUT"/>
    </root>
    <logger name="org.eclipse.jetty" level="INFO"/>
    <logger name="io.netty" level="INFO"/>
    <logger name="org.apache.kafka" level="WARN"/>
    <logger name="org.apache.kafka.common.security" level="WARN"/>
    <logger name="org.apache.kafka.common.metrics" level="WARN"/>
    <logger name="org.apache.kafka.common.metrics" level="INFO"/>
    <logger name="org.apache.parquet" level="INFO"/>
    <logger name="parquet" level="INFO"/>
    <logger name="org.apache.hadoop" level="INFO"/>
</configuration>
